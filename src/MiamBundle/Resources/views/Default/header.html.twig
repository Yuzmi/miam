{% spaceless %}
	<header>
		{% block preLink %}{% endblock %}
		{% if app.user %}
			<a href="{{ path('index') }}" title="{{ "My feeds"|trans({}, 'header') }}">
				<i class="fa fa-list"></i>
				<span class="label">{{ "My feeds"|trans({}, 'header') }}</span>
			</a>
		{% endif %}
		<a href="{{ path('catalog') }}" title="{{ "Catalog"|trans({}, 'header') }}">
			<i class="fa fa-book"></i>
			<span class="label">{{ "Catalog"|trans({}, 'header') }}</span>
		</a>
		{% if app.user %}
			<a href="{{ path('manager') }}" title="{{ "Manager"|trans({}, 'header') }}">
				<i class="fa fa-cog"></i>
				<span class="label">{{ "Manager"|trans({}, 'header') }}</span>
			</a>
			{% if is_granted('ROLE_ADMIN') %}
				<a href="{{ path('admin') }}" title="{{ "Admin"|trans({}, 'header') }}">
					<i class="fa fa-lock"></i>
					<span class="label">{{ "Admin"|trans({}, 'header') }}</span>
				</a>
			{% endif %}
		{% endif %}
		<span class="rightSide">
			{% if app.user %}
				<a href="{{ path('logout') }}" title="{{ "Log out"|trans({}, 'header') }}">
					<i class="fa fa-sign-out"></i>
					<span class="label">{{ "Log out"|trans({}, 'header') }}</span>
				</a>
			{% else %}
				<a href="{{ path('login') }}" title="{{ "Log in"|trans({}, 'header') }}">
					<i class="fa fa-sign-in"></i>
					<span class="label">{{ "Log in"|trans({}, 'header') }}</span>
				</a>
			{% endif %}
		</span>
	</header>
{% endspaceless %}